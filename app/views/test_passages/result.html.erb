<% if @test_passage.successful? %>
  <h2 class='heading heading_success'>The <%= @test_passage.test.title %> Test was Successfuly Completed</h2>
<% else %>
  <h2 class='heading heading_wrong'>The <%= @test_passage.test.title %> Test wasn't Completed</h2>
<% end %>
<p>
  Correct: <%= @test_passage.correct_questions %> from <%= @test_passage.test.questions.count %> | <%= @test_passage.complete_percentage %>%
</p>

<%= link_to 'Tests', tests_path %>